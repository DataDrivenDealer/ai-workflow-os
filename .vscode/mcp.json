{
  "mcpServers": {
    "ai-workflow-os": {
      "command": "python",
      "args": ["-m", "kernel.mcp_stdio"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    }
  }
}
